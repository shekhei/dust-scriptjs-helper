{
  "name": "dust-scriptjs-helper",
  "version": "0.0.3",
  "description": "Just a group of simple dustjs helpers for loading with scriptjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "cd src; ../node_modules/.bin/babel ./*.js -d ../dist",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shekhei/dust-scriptjs-helper.git"
  },
  "keywords": [
    "dustjs",
    "scriptjs",
    "helper"
  ],
  "author": "Wong Shek Hei Felix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shekhei/dust-scriptjs-helper/issues"
  },
  "homepage": "https://github.com/shekhei/dust-scriptjs-helper#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-istanbul": "^0.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "glob": "^7.0.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5"
  },
  "peerDependencies": {
    "dustjs-linkedin": "^2.7.0",
    "dustjs-helpers": "^1.7.0"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "eventemitter2": "^1.0.0"
  }
}
